    "cron-parser": "^5.1.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase-admin": "^13.2.0",
    "fs": "^0.0.1-security",
    "mysql2": "^3.14.0",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.3",
