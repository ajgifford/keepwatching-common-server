{
  "name": "@ajgifford/keepwatching-common-server",
  "version": "0.8.0",
  "description": "Common utilities and services for KeepWatching API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema.d.ts",
      "import": "./dist/schema.js",
      "require": "./dist/schema.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js",
      "require": "./dist/config.js"
    },
    "./services": {
      "types": "./dist/services.d.ts",
      "import": "./dist/services.js",
      "require": "./dist/services.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js",
      "require": "./dist/logger.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js",
      "require": "./dist/middleware.js"
    },
    "./testing": {
      "types": "./dist/testing/index.d.ts",
      "import": "./dist/testing/index.js",
      "require": "./dist/testing/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "rebuild": "yarn clean && yarn build",
    "prepare": "yarn build",
    "test": "rimraf node_modules/.vite && vitest run",
    "test:coverage": "rimraf node_modules/.vite && vitest run --coverage",
    "test:forceCoverage": "vitest run --coverage --reporter=verbose --passWithNoTests",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --config ./.prettierrc.cjs . --write",
    "format:check": "prettier --config ./.prettierrc.cjs . --check",
    "type-check": "tsc --noEmit",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajgifford/keepwatching-common-server.git"
  },
  "author": "Gifford Family Dev",
  "license": "ISC",
  "dependencies": {
    "@ajgifford/keepwatching-types": "^0.7.2",
    "axios": "1.12.2",
    "axios-retry": "^4.5.0",
    "cron-parser": "5.4.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase-admin": "13.5.0",
    "fs": "^0.0.1-security",
    "ioredis": "^5.8.2",
    "mysql2": "3.15.2",
    "node-cache": "^5.1.2",
    "node-cron": "4.2.1",
    "nodemailer": "7.0.9",
    "socket.io": "^4.8.1",
    "streaming-availability": "^4.4.0",
    "uuid": "^11.1.0",
    "winston": "3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/express": "5.0.3",
    "@types/node": "^22.14.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/uuid": "11.0.0",
    "@vitest/coverage-v8": "4.0.7",
    "@vitest/ui": "^4.0.7",
    "eslint": "9.37.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-react": "^7.37.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.46.1",
    "vite": "^7.2.1",
    "vitest": "^4.0.7"
  },
  "resolutions": {
    "punycode": "^2.3.1",
    "node-fetch": "2.7.0",
    "whatwg-url": "14.1.1"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
